<h2> Welcome to the To Do Lists app! </h2>
<br />

<p><%= link_to 'New List', new_list_path %></p>
<% @lists.each do |list| %>
  <h2><%= list.title %></h2>
  <%= link_to 'View', list_path(list) %>
  <%= link_to 'Edit', edit_list_path(list) %>
    <%= link_to 'Delete', list, method: :delete %>
<% end %>